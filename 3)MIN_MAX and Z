# Given data
data <- c(200, 300, 400, 600, 1000)

# Min-Max Normalization
min_val <- min(data)
max_val <- max(data)
min_max_norm <- (data - min_val) / (max_val - min_val)
min_max_norm

# Mean and Standard Deviation
mean_val <- mean(data)
sd_val <- sd(data)

# Z-Score Normalization
z_score_norm <- (data - mean_val) / sd_val
z_score_norm
